            },
            ".field-input": {
              "border": "none",
              "color": "@inputText",
              "&::-webkit-input-placeholder": {
                "color": "@inputText"
              },
              "&::-moz-placeholder": {
                "color": "@inputText"
              },
              "&:-moz-placeholder": {
                "color": "@inputText"
              },
              "&:-ms-input-placeholder": {
